<table class="table">
    <thead>
        <tr>
            <th scope="col">S.N.</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Contact</th>
            <th scope="col">Resume</th>
        </tr>
    </thead>
    <tbody>
        <% applicants.forEach((applicant, index)=>{ %>
            <tr>
                <th scope="row">
                    <%= index+1 %>
                </th>
                <td>
                    <%= applicant.name %>
                </td>
                <td>
                    <%= applicant.email %>
                </td>
                <td>
                    <%= applicant.contact %>
                </td>
                <td><a href="<%= applicant.resume %>" class="btn btn-primary">View</a></td>
            </tr>
            <% }) %>
    </tbody>
</table>